    <tbody>
        #foreach( $smData in $smDataList )
        <tr>
            <td>$smData.status</td>
            <td>$smData.name</td>
            <td>$smData.marketData.get("TEAM")</td>
            <td>$smData.marketData.get("PRICE")</td>
            <td>$smData.marketData.get("KEEP_BROKER")</td>
            <td>$smData.marketData.get("LAST_THREE_VAL")</td>
            <td>$smData.marketData.get("MVP")</td>
            <td style="background-color:#$smData.marketData.get("COLOR")">$smData.marketData.get("RANKING")</td>
            <td>$smData.position</td>
            <td>$smData.score</td>

        </tr>
      #end
    </tbody>
