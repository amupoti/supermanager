<!DOCTYPE html>
<html>
<head>
    <script type="text/javascript" src="/js/jquery-1.11.3.min.js"></script>
    <script type="text/javascript" src="/js/jquery.tablesorter.js"></script>
    <link rel="stylesheet" href="/css/style.css" type="text/css" id="" media="print, projection, screen">
<script>
$(document).ready(function()
    {
        $("#myTable").tablesorter();
    }
);

</script>
</head>
<body>
    <h1>Supermanager</h1>

    <table id="myTable" class="tablesorter">
    <thead>
    <tr>
        <th>Jugador</th>
        <th>Val-L</th>
        <th>Val-V</th>
        <th>Broker =</th>
        <th>Media Equipo</th>
        <th>Media Rec VS</th>
        <th>Media Equipo L/V</th>
        <th>Media Rec VS L/V</th>
        <th>Cal-Corto</th>
        <th>Cal-Medio</th>
        <th>Cal-Largo</th>
        <th>Equipo</th>
    </tr>
    </thead>
    <tbody>
         #foreach( $smData in $smDataList )
        <tr>
            <td>$smData.playerId</td>
            <td>$smData.playerLocalVal</td>
            <td>$smData.playerVisitorVal</td>
            <td>$smData.keepBroker</td>
            <td>$smData.teamVal</td>
            <td>$smData.otherTeamReceivedVal</td>
            <td>$smData.teamValAsLV</td>
            <td>$smData.otherTeamReceivedValAsLV</td>
            <td>$smData.calendarBoostShort</td>
            <td>$smData.calendarBoostMedium</td>
            <td>$smData.calendarBoostLong</td>
            <td>$smData.teamName</td>
        </tr>
      #end
    </tbody>
    </table>



</body>
</html>



